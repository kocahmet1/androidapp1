# Gemini API Key - Get one from https://makersuite.google.com/app/apikey
EXPO_PUBLIC_GEMINI_API_KEY=your_api_key_here

# Current Firebase Configuration (for Web/Internal Use)
EXPO_PUBLIC_FIREBASE_API_KEY=your_current_api_key
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your_current_project.firebaseapp.com
EXPO_PUBLIC_FIREBASE_DATABASE_URL=https://your_current_project-default-rtdb.firebaseio.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your_current_project
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your_current_project.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_current_sender_id
EXPO_PUBLIC_FIREBASE_APP_ID=your_current_app_id
EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID=your_current_measurement_id

# Android Firebase Configuration (for Android Store App)
EXPO_PUBLIC_ANDROID_FIREBASE_API_KEY=your_android_api_key
EXPO_PUBLIC_ANDROID_FIREBASE_AUTH_DOMAIN=your_android_project.firebaseapp.com
EXPO_PUBLIC_ANDROID_FIREBASE_DATABASE_URL=https://your_android_project-default-rtdb.firebaseio.com
EXPO_PUBLIC_ANDROID_FIREBASE_PROJECT_ID=your_android_project
EXPO_PUBLIC_ANDROID_FIREBASE_STORAGE_BUCKET=your_android_project.appspot.com
EXPO_PUBLIC_ANDROID_FIREBASE_MESSAGING_SENDER_ID=your_android_sender_id
EXPO_PUBLIC_ANDROID_FIREBASE_APP_ID=your_android_app_id
EXPO_PUBLIC_ANDROID_FIREBASE_MEASUREMENT_ID=your_android_measurement_id

# Environment Switch - Set to 'true' to use Android Firebase configuration
EXPO_PUBLIC_USE_ANDROID_CONFIG=false
